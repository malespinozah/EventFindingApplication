@model EventFindingApplication.Models.DetailsCity
@{
    ViewBag.Title = "Details";
}

<h2>Events in @Model.SelectedCity.CityName City</h2>

<ol>
    @foreach (var Event in Model.KeptEvents)
    {

        <li>
            <a href="/Event/Details/@Event.EventId" class="citylist">@Event.EventName</a>
        </li>

    }
</ol>

<div>
    <a href="/City/List" class="goback">Go back to list</a>
</div>
